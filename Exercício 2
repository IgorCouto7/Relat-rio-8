class Heroi:
    def __init__(self, nome: str, funcao: str):
        self.nome = nome
        self.funcao = funcao

    def usar_ultimate(self):
        raise NotImplementedError("Este m√©todo deve ser implementado pelas subclasses.")


class Tanque(Heroi):
    def __init__(self, nome: str):
        super().__init__(nome, "Tanque")

    def usar_ultimate(self):
        print(f"{self.nome} usa sua ultimate: cria um escudo gigante para proteger o time!")


class Dano(Heroi):
    def __init__(self, nome: str):
        super().__init__(nome, "Dano")

    def usar_ultimate(self):
        print(f"{self.nome} usa sua ultimate: dispara uma rajada devastadora de tiros!")


# Main
herois = [
    Tanque("Reinhardt"),
    Dano("Soldado 76"),
    Dano("Tracer"),
    Tanque("Winston")
]

for heroi in herois:
    heroi.usar_ultimate()
